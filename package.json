    "redux-persist": "^5.4.0",
    "redux-thunk": "^2.0.0",
    "reselect": "^3.0.1",
    "reset-css": "^2.2.0"
  },
  "scripts": {
    "start": "npm-run-all build:css --parallel watch:css start:ts",
    "build": "npm-run-all build:css build:ts",
    "lint": "npm-run-all lint:prettier lint:ts",
    "test": "npm-run-all build:css test:ts",
    "start:ts": "react-scripts-ts start",
    "build:ts": "react-scripts-ts build",
    "build:css": "node-sass-chokidar --quiet src/ -o src/",
    "watch:css": "node-sass-chokidar --quiet src/ -o src/ --watch --recursive",
    "lint:ts": "tslint --exclude '**/*.d.ts' 'src/**/*.ts' 'src/**/*.tsx'",
    "lint:prettier": "prettier --list-different 'src/**/*.ts' 'src/**/*.tsx'",
    "fix-lint:prettier": "prettier --write 'src/**/*.ts' 'src/**/*.tsx'",
    "test:ts": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.5",
    "@types/faker": "^4.1.1",
    "@types/history": "^4.6.2",
    "@types/jest": "^21.1.5",
    "@types/node": "^8.0.47",
