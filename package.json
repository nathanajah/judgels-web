    "@types/redux-form": "^7.0.0",
    "@types/redux-mock-store": "^0.0.12",
    "@types/redux-thunk": "^2.1.0",
    "@types/semantic-ui": "^2.2.5",
    "@types/slick-carousel": "^1.6.32",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "prettier": "1.9.2",
    "redux-mock-store": "^1.3.0",
    "tslint-config-prettier": "^1.6.0"
  },
  "resolutions": {
    "@types/react": "^16.0.0"
